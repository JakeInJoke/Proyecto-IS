<div class="field is-fullwidth">
  <h1 class="title is-3 mt-5">Resultados</h1>
</div>
<div class="container is-fullwidth">
  <% if (socio.length > 0) { %>
  <table class="table is-bordered is-fullwidth">
    <thead>
      <tr class="has-text-centered">
        <th scope="col">Codigo</th>
        <th scope="col">Nombre</th>
        <th scope="col">Negocio</th>
        <th scope="col">Membresia</th>
        <th scope="col">Pagos</th>
        <th scope="col">Accion</th>
      </tr>
    </thead>
    <tbody>
      <% socio.forEach((s, index) => { %>
      <tr>
        <td class="has-text-weight-bold is-vcentered"><%= s.codigo_socio %></td>
        <td class="is-vcentered"><%= s.nombre_socio %></td>
        <td class="is-vcentered"><%= s.nombre_compania %></td>
        <td class="is-vcentered"><%= s.membresia_socio %></td>
        <td class="is-vcentered"><%= s.pagos_atrasados %></td>
        <td class="is-vcentered has-text-centered">
          <a
            href="/addasister/<%= s.id_socio %>/<%= evento.id_evento%>"
            rel="noopener"
            class="button is-primary mr-2"
            >Agregar</a
          >
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <% ;} else { %>
  <p class="text-center">No hay socio.</p>
  <% ;} %>
</div>
